{
  "pipeline": {
    "name": "inference"
  },
  "transform": {
    "image": "pachyderm/iris-infer:julia",
    "cmd": [
	"julia",
	"/infer.jl",
	"/pfs/model/model.jld",
	"/pfs/attributes/",
	"/pfs/out/"
    ]
  },
  "parallelism_spec": {
    "constant": "5"
  },
  "input": {
    "cross": [
      {
        "atom": {
	  "repo": "attributes",
	  "glob": "/*"
	}
      },
      {
	"atom": {
	  "repo": "model",
	  "glob": "/"
	}
      }
    ]
  }
}
